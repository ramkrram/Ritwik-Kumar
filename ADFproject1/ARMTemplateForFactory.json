						"dependsOn": [],
						"policy": {
							"timeout": "7.00:00:00",
							"retry": 0,
							"retryIntervalInSeconds": 30,
							"secureOutput": false,
							"secureInput": false
						},
						"userProperties": [],
						"typeProperties": {
							"source": {
								"type": "SqlSource",
								"sqlReaderStoredProcedureName": "[concat('[', 'dbo].[all_tables_name]')]"
							},
							"dataset": {
								"referenceName": "get_tables_name_landing",
								"type": "DatasetReference",
								"parameters": {}
							},
							"firstRowOnly": false
						}
					},
					{
						"name": "get_landing_table_nm",
						"type": "ForEach",
						"dependsOn": [
							{
								"activity": "get_src_files",
								"dependencyConditions": [
