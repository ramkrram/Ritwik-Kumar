			"type": "string",
			"defaultValue": "cloud-shell-storage-centralindia"
		},
		"Local_laptop_linkservice_properties_typeProperties_userId": {
			"type": "string",
			"defaultValue": "ramkr4151"
		},
		"local_file_system_properties_typeProperties_userId": {
			"type": "string",
			"defaultValue": "ramkr4151"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas Token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeAnalytics_servicePrincipalKey": {
						"value": "[parameters('AzureDataLakeAnalytics_servicePrincipalKey')]"
					},
