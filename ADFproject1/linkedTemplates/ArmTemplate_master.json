		"ADLS_link_service_properties_typeProperties_subscriptionId": {
			"type": "string",
			"defaultValue": "844b9775-cc7c-4850-bd18-7fdf7bbca33d"
		},
		"ADLS_link_service_properties_typeProperties_resourceGroupName": {
			"type": "string",
			"defaultValue": "cloud-shell-storage-centralindia"
		},
		"AzureDataLakeAnalytics_properties_typeProperties_accountName": {
			"type": "string",
			"defaultValue": "adlstcs"
		},
		"AzureDataLakeAnalytics_properties_typeProperties_servicePrincipalId": {
			"type": "string",
			"defaultValue": "eb8492d0-54d7-4161-b7a2-e6f7556db3f9"
		},
