{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory Name",
			"defaultValue": "ADFproject1"
		},
		"AzureDataLakeAnalytics_servicePrincipalKey": {
			"type": "secureString",
			"metadata": "Secure string for 'servicePrincipalKey' of 'AzureDataLakeAnalytics'"
		},
		"AzureSqlDatabase_landing_DB_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabase_landing_DB'"
		},
		"Local_db_landing_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Local_db_landing'"
		},
		"Local_laptop_linkservice_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'Local_laptop_linkservice'"
		},
		"SqlServer_local_DB_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'SqlServer_local_DB'"
		},
		"local_file_system_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'local_file_system'"
		},
		"AzureDataLakeStoreFile1_properties_typeProperties_fileName": {
			"type": "string",
			"defaultValue": ""
		},
		"AzureDataLakeStoreFile1_properties_typeProperties_folderPath": {
			"type": "string",
			"defaultValue": "development/landing"
		},
		"AzureDataLakeStoreFile2_properties_typeProperties_fileName": {
			"type": "string",
			"defaultValue": ""
		},
		"AzureDataLakeStoreFile2_properties_typeProperties_folderPath": {
			"type": "string",
			"defaultValue": "development/landing"
		},
		"AzureDataLakeStoreFile3_properties_typeProperties_fileName": {
			"type": "string",
			"defaultValue": "filter_data.txt"
		},
		"AzureDataLakeStoreFile3_properties_typeProperties_folderPath": {
			"type": "string",
			"defaultValue": "development/output"
		},
		"FileShare1_properties_typeProperties_fileName": {
			"type": "string",
			"defaultValue": "@concat(activity('local_input_file_name').output.value),'.csv')"
		},
		"FileShare1_properties_typeProperties_folderPath": {
			"type": "string",
			"defaultValue": ""
		},
		"FileShare_local_properties_typeProperties_fileName": {
			"type": "string",
			"defaultValue": "@activity('local_input_file_name').output.value"
		},
		"get_file_from_local_properties_typeProperties_fileName": {
			"type": "string",
			"defaultValue": "@concat(item().TABLENAME,'.csv')"
		},
		"metadata_input_detls_properties_typeProperties_fileName": {
			"type": "string",
			"defaultValue": "@concat(item().name,'.csv')"
		},
		"metadata_input_detls_properties_typeProperties_folderPath": {
			"type": "string",
			"defaultValue": "development/landing"
		},
		"put_file_adls_properties_typeProperties_fileName": {
			"type": "string",
			"defaultValue": "@concat(item().TABLENAME,'.csv')"
		},
		"put_file_adls_properties_typeProperties_folderPath": {
			"type": "string",
			"defaultValue": "development/landing"
		},
		"ADLS_link_service_properties_typeProperties_dataLakeStoreUri": {
			"type": "string",
			"defaultValue": "https://adlsgen1storage.azuredatalakestore.net/webhdfs/v1"
		},
		"ADLS_link_service_properties_typeProperties_tenant": {
			"type": "string",
			"defaultValue": "69cf0f09-b27a-4183-b3cc-3f3b8a180245"
