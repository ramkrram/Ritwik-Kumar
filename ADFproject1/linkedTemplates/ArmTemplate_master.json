					},
					"AzureDataLakeAnalytics_properties_typeProperties_tenant": {
						"value": "[parameters('AzureDataLakeAnalytics_properties_typeProperties_tenant')]"
					},
					"AzureDataLakeAnalytics_properties_typeProperties_subscriptionId": {
						"value": "[parameters('AzureDataLakeAnalytics_properties_typeProperties_subscriptionId')]"
					},
					"AzureDataLakeAnalytics_properties_typeProperties_resourceGroupName": {
						"value": "[parameters('AzureDataLakeAnalytics_properties_typeProperties_resourceGroupName')]"
					},
					"ADLS_link_service_properties_typeProperties_dataLakeStoreUri": {
						"value": "[parameters('ADLS_link_service_properties_typeProperties_dataLakeStoreUri')]"
					},
					"ADLS_link_service_properties_typeProperties_tenant": {
						"value": "[parameters('ADLS_link_service_properties_typeProperties_tenant')]"
					},
					"ADLS_link_service_properties_typeProperties_subscriptionId": {
						"value": "[parameters('ADLS_link_service_properties_typeProperties_subscriptionId')]"
					},
					"ADLS_link_service_properties_typeProperties_resourceGroupName": {
						"value": "[parameters('ADLS_link_service_properties_typeProperties_resourceGroupName')]"
					},
					"Local_laptop_linkservice_properties_typeProperties_userId": {
						"value": "[parameters('Local_laptop_linkservice_properties_typeProperties_userId')]"
					},
					"local_file_system_properties_typeProperties_userId": {
						"value": "[parameters('local_file_system_properties_typeProperties_userId')]"
					},
					"AzureDataLakeStoreFile1_properties_typeProperties_fileName": {
						"value": "[parameters('AzureDataLakeStoreFile1_properties_typeProperties_fileName')]"
					},
					"AzureDataLakeStoreFile1_properties_typeProperties_folderPath": {
						"value": "[parameters('AzureDataLakeStoreFile1_properties_typeProperties_folderPath')]"
					},
					"AzureDataLakeStoreFile2_properties_typeProperties_fileName": {
						"value": "[parameters('AzureDataLakeStoreFile2_properties_typeProperties_fileName')]"
					},
					"AzureDataLakeStoreFile2_properties_typeProperties_folderPath": {
						"value": "[parameters('AzureDataLakeStoreFile2_properties_typeProperties_folderPath')]"
					},
					"AzureDataLakeStoreFile3_properties_typeProperties_fileName": {
						"value": "[parameters('AzureDataLakeStoreFile3_properties_typeProperties_fileName')]"
					},
					"AzureDataLakeStoreFile3_properties_typeProperties_folderPath": {
						"value": "[parameters('AzureDataLakeStoreFile3_properties_typeProperties_folderPath')]"
					},
					"FileShare1_properties_typeProperties_fileName": {
						"value": "[parameters('FileShare1_properties_typeProperties_fileName')]"
					},
					"FileShare1_properties_typeProperties_folderPath": {
						"value": "[parameters('FileShare1_properties_typeProperties_folderPath')]"
					},
					"FileShare_local_properties_typeProperties_fileName": {
						"value": "[parameters('FileShare_local_properties_typeProperties_fileName')]"
					},
					"get_file_from_local_properties_typeProperties_fileName": {
						"value": "[parameters('get_file_from_local_properties_typeProperties_fileName')]"
					},
					"metadata_input_detls_properties_typeProperties_fileName": {
						"value": "[parameters('metadata_input_detls_properties_typeProperties_fileName')]"
